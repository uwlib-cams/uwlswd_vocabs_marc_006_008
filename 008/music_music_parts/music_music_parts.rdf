<?xml version="1.0" encoding="UTF-8"?> <rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:schema="https://schema.org/" xmlns:skos="http://www.w3.org/2004/02/skos/core#"> <!-- Scheme: MARC21-007: Music parts --> <skos:ConceptScheme rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46"> <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/> <dct:title>Music: music parts</dct:title> <dct:alternative>MARC21 008/21 values for music expressed using RDF</dct:alternative> <dct:description>Indicates whether the item being cataloged is, or contains parts.</dct:description> <skos:scopeNote xml:lang="en">Is not used to indicate that parts may exist elsewhere.</skos:scopeNote> <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/> <dct:creator rdf:resource="http://viaf.org/viaf/139541794"/> <dct:contributor rdf:resource="http://viaf.org/viaf/151962300"/> <dc:contributor>Metadata Management Associates</dc:contributor> <dct:issued>2023</dct:issued> <dc:language>en</dc:language> <dct:source rdf:resource="https://www.loc.gov/marc/bibliographic/bd008.html"/> <dct:source rdf:resource="http://marc21rdf.info"/> <dct:provenance rdf:resource="https://doi.org/10.6069/uwlswd.2e2b-y833"/> <dct:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0"/> <schema:version>1-0-0</schema:version> </skos:ConceptScheme> <!-- Concept: instrumental and vocal parts --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46#d"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.ywjs-vr46"/> <skos:prefLabel xml:lang="en">instrumental and vocal parts</skos:prefLabel> <skos:definition xml:lang="en">Both instrumental and vocal parts are present.</skos:definition> <skos:notation xml:lang="en">d</skos:notation> </skos:Concept> <!-- Concept: instrumental parts --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46#e"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.ywjs-vr46"/> <skos:prefLabel xml:lang="en">instrumental parts</skos:prefLabel> <skos:definition xml:lang="en">Instrumental parts are present.</skos:definition> <skos:notation xml:lang="en">e</skos:notation> </skos:Concept> <!-- Concept: no parts in hand or not specified --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46#pound"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.ywjs-vr46"/> <skos:prefLabel xml:lang="en">no parts in hand or not specified</skos:prefLabel> <skos:definition xml:lang="en">No parts in hand or the musical parts are not specified.</skos:definition> <skos:notation xml:lang="en">#</skos:notation> </skos:Concept> <!-- Concept: not applicable --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46#n"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.ywjs-vr46"/> <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel> <skos:definition xml:lang="en">Item is not notated music.</skos:definition> <skos:notation xml:lang="en">n</skos:notation> </skos:Concept> <!-- Concept: parts exist [obsolete] --> <skos:Concept rdf:about="http://fakeIRI.edu/terms/mapspe#ax"> <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/mapspe#"/> <skos:prefLabel xml:lang="en">parts exist [obsolete]</skos:prefLabel> <skos:notation xml:lang="en">a</skos:notation> </skos:Concept> <!-- Concept: unknown --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46#u"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.ywjs-vr46"/> <skos:prefLabel xml:lang="en">unknown</skos:prefLabel> <skos:definition xml:lang="en">Unknown whether the item being cataloged contains parts.</skos:definition> <skos:notation xml:lang="en">u</skos:notation> <skos:scopeNote xml:lang="en">May be used for records created without examining the item, such as retrospective conversion from a printed card.</skos:scopeNote> </skos:Concept> <!-- Concept: vocal parts --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.ywjs-vr46#f"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.ywjs-vr46"/> <skos:prefLabel xml:lang="en">vocal parts</skos:prefLabel> <skos:definition xml:lang="en">Vocal parts are present.</skos:definition> <skos:notation xml:lang="en">f</skos:notation> </skos:Concept> </rdf:RDF>