<?xml version="1.0" encoding="UTF-8"?> <rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:schema="https://schema.org/" xmlns:skos="http://www.w3.org/2004/02/skos/core#"> <!-- Scheme: MARC21-008: Nature of entire work of continuing resource --> <skos:ConceptScheme rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647"> <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/> <dct:title>Continuing: nature of entire work</dct:title> <dct:alternative>MARC21 008/24 values for continuing resources expressed using RDF</dct:alternative> <dct:description>Indicates the nature of an item if it consists entirely of a certain type of material.</dct:description> <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/> <dct:source rdf:resource="https://www.loc.gov/marc/bibliographic/bd008.html"/> <dct:source rdf:resource="http://marc21rdf.info"/> <dct:provenance rdf:resource="https://doi.org/10.6069/uwlswd.2e2b-y833"/> <dct:creator rdf:resource="http://viaf.org/viaf/139541794"/> <dct:contributor rdf:resource="http://viaf.org/viaf/151962300"/> <dc:contributor>Metadata Management Associates</dc:contributor> <dct:issued>2023</dct:issued> <dc:language>en</dc:language> <dct:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0"/> <schema:version>1-0-0</schema:version> </skos:ConceptScheme> <!-- Concept: abstracts or summaries --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#a"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">abstracts or summaries</skos:prefLabel> <skos:definition xml:lang="en">Abstracts or summaries of other publications.</skos:definition> <skos:notation xml:lang="en">a</skos:notation> </skos:Concept> <!-- Concept: bibliographies --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#b"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">bibliographies</skos:prefLabel> <skos:notation xml:lang="en">b</skos:notation> </skos:Concept> <!-- Concept: biography --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#h"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">biography</skos:prefLabel> <skos:definition xml:lang="en">Biographical material, whether autobiography, individual biography, or collective biography.</skos:definition> <skos:notation xml:lang="en">h</skos:notation> <skos:scopeNote xml:lang="en">Not used for genealogy.</skos:scopeNote> </skos:Concept> <!-- Concept: calendars --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#5"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">calendars</skos:prefLabel> <skos:definition xml:lang="en">Published systems of organizing days. These may be academic calendars or almanacs, calendars published by bodies, such as labor organizations, library associations, etc.</skos:definition> <skos:notation xml:lang="en">5</skos:notation> </skos:Concept> <!-- Concept: catalogs --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#c"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">catalogs</skos:prefLabel> <skos:definition xml:lang="en">List of items in a collection, such as a collection of books, a collection of art objects, etc.</skos:definition> <skos:notation xml:lang="en">c</skos:notation> </skos:Concept> <!-- Concept: comics or graphic novels --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#6"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">comics or graphic novels</skos:prefLabel> <skos:definition xml:lang="en">Instances of "sequential art" in which a story (whether fact or fiction) is told primarily through a set of images (often in the form of multiple "panels" per page) presented concurrently but meant to be "read" sequentially by the viewer. The accompanying narrative and/or dialog text, when it occurs, works integrally with the images to tell the story.</skos:definition> <skos:notation xml:lang="en">6</skos:notation> </skos:Concept> <!-- Concept: dictionaries --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#d"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">dictionaries</skos:prefLabel> <skos:notation xml:lang="en">d</skos:notation> <skos:scopeNote xml:lang="en">Also includes glossaries or gazetteers. Not used for concordances or serial biographical dictionaries.</skos:scopeNote> </skos:Concept> <!-- Concept: directories --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#r"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">directories</skos:prefLabel> <skos:definition xml:lang="en">Directory or register of persons or corporate bodies.</skos:definition> <skos:notation xml:lang="en">r</skos:notation> <skos:scopeNote xml:lang="en">Not used for serial biographical dictionaries.</skos:scopeNote> </skos:Concept> <!-- Concept: discographies --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#k"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">discographies</skos:prefLabel> <skos:definition xml:lang="en">Discography or other bibliography of recorded sound.</skos:definition> <skos:notation xml:lang="en">k</skos:notation> </skos:Concept> <!-- Concept: encyclopedias --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#e"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">encyclopedias</skos:prefLabel> <skos:definition xml:lang="en">Encyclopedia or an encyclopedic treatment of a specific topic.</skos:definition> <skos:notation xml:lang="en">e</skos:notation> </skos:Concept> <!-- Concept: filmographies --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#q"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">filmographies</skos:prefLabel> <skos:definition xml:lang="en">Filmography or other bibliography of moving images.</skos:definition> <skos:notation xml:lang="en">q</skos:notation> </skos:Concept> <!-- Concept: handbooks --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#f"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">handbooks</skos:prefLabel> <skos:notation xml:lang="en">f</skos:notation> </skos:Concept> <!-- Concept: indexes --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#i"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">indexes</skos:prefLabel> <skos:definition xml:lang="en">Index to bibliographical material other than itself (e.g., an indexing journal).</skos:definition> <skos:notation xml:lang="en">i</skos:notation> </skos:Concept> <!-- Concept: law reports and digests --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#w"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">law reports and digests</skos:prefLabel> <skos:definition xml:lang="en">Texts of decisions of courts or administrative agencies.</skos:definition> <skos:notation xml:lang="en">w</skos:notation> <skos:scopeNote xml:lang="en">Also used when a work consists of texts of digests of such decisions.</skos:scopeNote> </skos:Concept> <!-- Concept: legal articles --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#g"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">legal articles</skos:prefLabel> <skos:definition xml:lang="en">Substantive articles on legal topics, such as those published in law school reviews.</skos:definition> <skos:notation xml:lang="en">g</skos:notation> </skos:Concept> <!-- Concept: legal cases and case notes --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#v"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">legal cases and case notes</skos:prefLabel> <skos:definition xml:lang="en">Discussions, such as those in the case comments section of law school reviews, of particular legal cases that have been decided by, or that are pending before, courts or administrative agencies.</skos:definition> <skos:notation xml:lang="en">v</skos:notation> </skos:Concept> <!-- Concept: legal cases and case notes [obsolete] --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#nx"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">legal cases and case notes [obsolete]</skos:prefLabel> <skos:notation xml:lang="en">n</skos:notation> </skos:Concept> <!-- Concept: legislation --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#l"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">legislation</skos:prefLabel> <skos:definition xml:lang="en">Full or partial texts of enactments of legislative bodies or texts of rules and regulations issued by executive or administrative agencies.</skos:definition> <skos:notation xml:lang="en">l</skos:notation> <skos:scopeNote xml:lang="en">Also used when a work consists of texts of rules and regulations issued by executive or administrative agencies.</skos:scopeNote> </skos:Concept> <!-- Concept: not specified --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#pound"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">not specified</skos:prefLabel> <skos:definition xml:lang="en">Nature of the entire item is not specified.</skos:definition> <skos:notation xml:lang="en">#</skos:notation> </skos:Concept> <!-- Concept: programmed texts --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#p"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">programmed texts</skos:prefLabel> <skos:notation xml:lang="en">p</skos:notation> </skos:Concept> <!-- Concept: reviews --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#o"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">reviews</skos:prefLabel> <skos:definition xml:lang="en">Critical reviews of published or performed works (e.g., books, films, sound recordings, theater, etc.).</skos:definition> <skos:notation xml:lang="en">o</skos:notation> </skos:Concept> <!-- Concept: standards or specifications --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#u"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">standards or specifications</skos:prefLabel> <skos:definition xml:lang="en">Either an international, national, or industry standard or a specification which gives a precise statement of a process or service requirement.</skos:definition> <skos:notation xml:lang="en">u</skos:notation> </skos:Concept> <!-- Concept: statistics --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#s"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">statistics</skos:prefLabel> <skos:definition xml:lang="en">Collection of statistical data on a subject.</skos:definition> <skos:notation xml:lang="en">s</skos:notation> <skos:scopeNote xml:lang="en">Not used for works about statistical methodology.</skos:scopeNote> </skos:Concept> <!-- Concept: surveys of literature in a subject area --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#n"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">surveys of literature in a subject area</skos:prefLabel> <skos:definition xml:lang="en">Authored surveys that summarize what has been published about a subject. Usually has a list of references either in the body of the work or as a bibliography.</skos:definition> <skos:notation xml:lang="en">n</skos:notation> </skos:Concept> <!-- Concept: technical reports --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#t"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">technical reports</skos:prefLabel> <skos:definition xml:lang="en">The result of scientific investigation or technical development, testing, or evaluation, presented in a form suitable for dissemination to the technical community.</skos:definition> <skos:notation xml:lang="en">t</skos:notation> </skos:Concept> <!-- Concept: theses --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#m"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">theses</skos:prefLabel> <skos:definition xml:lang="en">Thesis, dissertation, or work identified as having been created to satisfy the requirements for an academic certification or degree.</skos:definition> <skos:notation xml:lang="en">m</skos:notation> </skos:Concept> <!-- Concept: treaties --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#z"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">treaties</skos:prefLabel> <skos:definition xml:lang="en">Treaties or accords negotiated between two or more parties to settle a disagreement, establish a relationship, grant rights, etc.</skos:definition> <skos:notation xml:lang="en">z</skos:notation> </skos:Concept> <!-- Concept: yearbook [obsolete] --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#yx"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">yearbook [obsolete]</skos:prefLabel> <skos:notation xml:lang="en">y</skos:notation> </skos:Concept> <!-- Concept: yearbooks --> <skos:Concept rdf:about="https://doi.org/10.6069/uwlswd.jfr5-z647#y"> <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.jfr5-z647"/> <skos:prefLabel xml:lang="en">yearbooks</skos:prefLabel> <skos:definition xml:lang="en">Reference publication issued on an annual or less frequent basis that contains articles summarizing the accomplishments or events of a particular year within a specific discipline or area of endeavor.</skos:definition> <skos:notation xml:lang="en">y</skos:notation> <skos:scopeNote xml:lang="en">Not used for annual reports, which are administrative overviews of an organization.</skos:scopeNote> </skos:Concept> </rdf:RDF>